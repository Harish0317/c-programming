#include <stdio.h>


void main() {
    int a,b,i;
    printf("enter a no of rows");
    scanf("%d",&a);
    printf("enter a no of column");
    scanf("%d",&b);
   
    
    
    for(i=0;i<a;i++)
    {
        for(int j=0;j<b;j++)
        {
        printf(" *");
        }
        printf("\n");
    }

    
}
